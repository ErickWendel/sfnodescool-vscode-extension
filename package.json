{
    "name": "wlist-nodeschool",
    "displayName": "wlist-nodeschool",
    "description": "",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.33.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.createTask",
                "title": "Nodeschool: New task",
                "category": "nodeschool"
            }
        ]
