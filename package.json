        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.42",
        "@types/moment": "^2.13.0",
        "@types/node": "^8.10.25",
        "tslint": "^5.8.0",
        "typescript": "^3.1.4",
        "vscode": "^1.1.33"
    },
    "dependencies": {
        "alarm": "^2.0.0",
        "moment": "^2.24.0",
        "sfx": "^0.1.1"
    }
}
